#include <iostream>
#include <cstdlib>
#include <fstream>
#include <ctime>
#define ARRAY_SIZE 10000
#define ELEMENT_RANGE ((1<<10)-1)

using namespace std;
int main(){
	srand(time(NULL));
	ofstream out;
	out.open("data.in");
	int size;//array size.
	int queries;	//number of queries
	size=(rand()%10000)+100;
	/*
	cout<<"Input array size"<<endl;
	cin>>size;
	*/
	out<<size<<endl;
	
	
	for(int i=0;i<size;i++){
		out<<rand()%ELEMENT_RANGE<<" ";
	}
	cout<<"Enter number of queries"<<endl;
	

	return 0;
}
